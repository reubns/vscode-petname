{
  "publisher": "reubns",
  "name": "petname",
  "displayName": "Petname",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.10.5",
  "description": "",
  "author": "Reuben <36566445+reubns@users.noreply.github.com>",
  "license": "MIT",
  "homepage": "https://github.com/reubens/vscode-petname#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/reubens/vscode-petname"
  },
  "bugs": {
    "url": "https://github.com/reubens/vscode-petname/issues"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "petname.generate",
        "title": "Generate",
        "category": "Petname"
      }
    ],
    "configuration": {
      "title": "Petname",
      "properties": {
        "petname.languages": {
          "type": "array",
          "default": [
            "plaintext",
            "typescript"
          ]
        },
        "petname.separator": {
          "type": "string",
          "default": "-",
          "description": "Separator between words"
        },
        "petname.words": {
          "type": "number",
          "default": 2,
          "description": "Number of words to generate"
        }
      }
    },
    "keybindings": [
      {
        "command": "petname.generate",
        "key": "ctrl+alt+p"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@antfu/ni": "^0.21.9",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.84.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "pnpm": "^8.10.5",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
